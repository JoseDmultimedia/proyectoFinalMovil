<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".revisarUsuarios">

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="723dp"
        android:layout_height="357dp"
        android:layout_x="-91dp"
        android:layout_y="-165dp"
        app:srcCompat="@drawable/backuser" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="101dp"
        android:layout_height="70dp"
        android:layout_x="20dp"
        android:layout_y="105dp"
        app:srcCompat="@drawable/search" />

    <EditText
        android:id="@+id/buscarU"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="66dp"
        android:layout_y="206dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="61dp" />

    <Button
        android:id="@+id/look"
        android:background="@drawable/btn_uno"
        android:textColor="@drawable/txt_btn_uno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="151dp"
        android:layout_y="273dp"
        android:onClick="mostrar"
        android:text="Buscar" />

    <ImageButton
        android:id="@+id/help"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="309dp"
        android:layout_y="203dp"
        app:srcCompat="@android:drawable/ic_menu_help" />

    <TextView
        android:id="@+id/idalsass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="120dp"
        android:layout_y="129dp"
        android:text="Buscar Usuarios"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="27dp" />

    <ListView
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="389dp"
        android:layout_x="-2dp"
        android:layout_y="339dp" />

</AbsoluteLayout>